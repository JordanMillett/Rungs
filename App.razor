<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(Components.Main.View)" />
    </Found>
    <NotFound>
        <PageTitle>ERROR</PageTitle>
        <LayoutView Layout="@typeof(Components.Main.View)">
            <p>ERROR</p>
        </LayoutView>
    </NotFound>
</Router>
